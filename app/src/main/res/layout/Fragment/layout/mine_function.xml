<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/pageBackground"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ServiceLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_less"
        android:background="@drawable/press_item"
        android:orientation="horizontal"
        android:padding="@dimen/padding_less2">

        <ImageView
            android:layout_width="@dimen/icon_size"
            android:layout_height="@dimen/icon_size"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/margin_less"
            android:layout_marginEnd="@dimen/right_margin"
            app:srcCompat="@drawable/mine_service" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="@string/mine_service"
            android:textColor="@color/black"
            android:textSize="@dimen/text_f1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_less"
        android:background="@drawable/shape_button_lines9"
        android:orientation="horizontal"
        android:padding="@dimen/padding_less2">

        <ImageView
            android:layout_width="@dimen/icon_size"
            android:layout_height="@dimen/icon_size"
            android:layout_marginStart="@dimen/margin_less"
            android:layout_marginEnd="@dimen/right_margin"
            app:srcCompat="@drawable/mine_case" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/mine_case"
            android:textColor="@color/black"
            android:textSize="@dimen/text_f1" />
    </LinearLayout>


    <LinearLayout
        android:id="@+id/physiqueLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_button_lines9"
        android:orientation="horizontal"
        android:padding="@dimen/padding_less2">

        <ImageView
            android:layout_width="@dimen/icon_size"
            android:layout_height="@dimen/icon_size"
            android:layout_marginStart="@dimen/margin_less"
            android:layout_marginEnd="@dimen/right_margin"
            app:srcCompat="@drawable/mine_physique" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/mine_physique"
            android:textColor="@color/black"
            android:textSize="@dimen/text_f1" />
    </LinearLayout>


